{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {
    "max-line-length": {
      "options": [200]
    }
  },
  "rules": {
      "quotemark": [true, "single"],
      "trailing-comma": [ false ],
      "max-line-length": {
        "options": [200]
      },
      "no-console": {
        "severity": "warning",
        "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
      },
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": false,
      "no-any": {
        "severity": "warning",
        "options": [true]
      }
  },
  "rulesDirectory": []
}